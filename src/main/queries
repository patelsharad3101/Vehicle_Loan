CREATE TABLE "GR4_USER_DETAILS" (
"GUD_ID" NUMBER(5) NOT NULL,
"GUD_NAME" VARCHAR2(50) NOT NULL,
"GUD_GENDER" VARCHAR2(20) NOT NULL,
"GUD_AGE" NUMBER(3) NOT NULL,
"GUD_MOBILE" NUMBER(10) NOT NUll,
"GUD_EMAIL" VARCHAR2(50) NOT NULL,
"GUD_PASSWORD" VARCHAR2(50) NOT NULL,
"GUD_ADDRESS" VARCHAR2(100) NOT NULL,
"GUD_STATE" VARCHAR2(50) NOT NULL,
"GUD_CITY" VARCHAR2(50) NOT NULL,
"GUD_PINCODE" NUMBER(6) NOT NUll,
CONSTRAINT "GR4_USER_DETAILS_PK" primary key ("GUD_ID")
);
